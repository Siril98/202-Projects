// File for Scented.h called Scented.cpp
#include "Scented.h"

Scented::Scented(string name,
		 double burnRate,
		 string flavor)
  : Candle(name, burnRate)
{
  m_flavor = flavor;
}

string Scented::GetFlavor()

{
  return m_flavor;
}

void Scented::SetFlavor(string flavor){
  m_flavor = flavor;
}

